{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "jasmine": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "func-names": 0,
    "no-unused-vars": 0,
    "import/no-useless-path-segments": 0,
    "comma-dangle": 0,
    "import/order": 0,
    "no-prototype-builtins": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "no-useless-return": 0,
    "eqeqeq": 0,
    "prefer-const": 0,
    "no-return-assign": 0,
    "no-shadow": 0,
    "class-methods-use-this": 0,
    "implicit-arrow-linebreak": 0,
    "no-empty-function": 0,
    "operator-linebreak": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0
  },
  "plugins": ["jasmine"]
}
